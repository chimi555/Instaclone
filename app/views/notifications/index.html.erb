<%= provide(:title, "通知一覧")%>
<% notifications = @notifications.where.not(visitor_id: current_user.id) %>
<div>
  <div class="notifications-title">通知</div>
  <% if notifications.exists? %>
    <div class="notifications-index">
      <%= render notifications %>
      <%= will_paginate %>
    </div>
  <% else %>
    <p>通知はありません</p>
  <% end %>
</div>